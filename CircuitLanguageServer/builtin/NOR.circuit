circuit NOR {
    inputs { a, b }
    outputs { y }

    lookup_tables {
        nor_table = {
            00 -> 1
            01 -> 0
            10 -> 0
            11 -> 0
        }
    }

    gates {
        lut = LookupTable("nor_table")
    }

    connections {
        a -> lut.in[0]
        b -> lut.in[1]
        lut.out -> y
    }
}