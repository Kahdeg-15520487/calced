circuit AND {
    inputs { a, b }
    outputs { y }

    lookup_tables {
        and_table = {
            00 -> 0
            01 -> 0
            10 -> 0
            11 -> 1
        }
    }

    gates {
        lut = LookupTable("and_table")
    }

    connections {
        a -> lut.in[0]
        b -> lut.in[1]
        lut.out -> y
    }
}