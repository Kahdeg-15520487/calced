{
  "name": "circuit-language-server",
  "displayName": "Circuit Language Server",
  "description": "Language server for Circuit DSL",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kahdeg-15520487/calced.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "circuit",
        "aliases": [
          "Circuit",
          "circuit"
        ],
        "extensions": [
          ".circuit"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "circuit",
        "scopeName": "source.circuit",
        "path": "./syntaxes/circuit.tmLanguage.json"
      }
    ],
    "configurationDefaults": {
      "editor.semanticHighlighting.enabled": true,
      "editor.semanticTokenColorCustomizations": {
        "rules": {
          "circuitKeyword": "#569CD6",
          "circuitOperator": "#d4d4d4",
          "circuitFunction": "#4EC9B0",
          "comment": "#6A9955",
          "string": "#CE9178",
          "identifier": "#9CDCFE"
        }
      }
    }
  },
  "files": [
    "out/**/*",
    "builtin/**/*",
    "syntaxes/**/*",
    "language-configuration.json",
    "bin/**/*",
    "LICENSE"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run copy-bin",
    "build": "vsce package",
    "compile": "webpack --mode production && webpack --mode production --config webpack.server.config.js",
    "watch": "webpack --mode development --watch",
    "copy-bin": "node scripts/copy-bin.js",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "vscode-languageclient": "^8.1.0",
    "vscode-languageserver": "^8.1.0",
    "vscode-languageserver-textdocument": "^1.0.8"
  },
  "devDependencies": {
    "@types/node": "18.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "@vscode/vsce": "^3.6.0",
    "eslint": "^8.34.0",
    "ts-loader": "^9.5.4",
    "typescript": "^4.9.5",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1"
  }
}
