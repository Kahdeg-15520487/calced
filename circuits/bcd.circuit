import "bcddecoder"
import "bcdencoder"

circuit testbcd{
    inputs { a[4] }
    outputs { b[4] }
    gates {
        encoder = Circuit("bcdencoder")
        decoder = Circuit("bcddecoder")
    }

    connections{
        a -> decoder.in
        decoder.out -> encoder.in
        encoder.out -> b
    }
}